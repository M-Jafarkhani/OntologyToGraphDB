<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <owl:Class rdf:about="http://dbpedia.org/ontology/Person">
        <rdfs:label>Person</rdfs:label>
        <rdfs:comment>A person</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://dbpedia.org/resource/Category:Presidents_of_the_United_States">
        <rdfs:label>President</rdfs:label>
        <rdfs:comment>President of the United States</rdfs:comment>
    </owl:Class>

    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdfs:label>Name</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of the person.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/subject">
        <rdfs:label>Subject</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Category:Presidents_of_the_United_States"/>
        <rdfs:comment>The subject of being US President.</rdfs:comment>
    </owl:ObjectProperty>

</rdf:RDF>

<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <owl:Class rdf:about="http://dbpedia.org/ontology/Star">
        <rdfs:label>Star</rdfs:label>
        <rdfs:comment>A Star</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://dbpedia.org/ontology/Constellation">
        <rdfs:label>Constellation</rdfs:label>
        <rdfs:comment>Astronomical constellation.</rdfs:comment>
    </owl:Class>

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
        <rdfs:label>Name</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Star" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
        <rdfs:comment>The label of the star.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://dbpedia.org/property/ageMyr">
        <rdfs:label>Age</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Star" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer" />
        <rdfs:comment>Age of the star</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://dbpedia.org/property/Luminosity">
        <rdfs:label>Luminosity</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Star" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double" />
        <rdfs:comment>Luminosity</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
        <rdfs:label>Title</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Constellation" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
        <rdfs:comment>The label of the Constellation.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://dbpedia.org/property/latmax">
        <rdfs:label>Lat Max</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Constellation" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer" />
        <rdfs:comment>Latmax of the Constellation</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://dbpedia.org/property/latmin">
        <rdfs:label>Lat Min</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Constellation" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer" />
        <rdfs:comment>Latmin of the Constellation</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/constell">
        <rdfs:label>Located In</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Star" />
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Constellation" />
        <rdfs:comment>Star's Constellation</rdfs:comment>
    </owl:ObjectProperty>

</rdf:RDF>
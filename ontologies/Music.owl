<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Define the Person class -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Person">
        <rdfs:label>Person</rdfs:label>
        <rdfs:comment>An individual human being.</rdfs:comment>
    </owl:Class>

    <!-- Define the Song class -->
    <owl:Class rdf:about="http://dbpedia.org/ontology/Song">
        <rdfs:label>Song</rdfs:label>
        <rdfs:comment>A Song.</rdfs:comment>
    </owl:Class>

    <!-- Define DatatypeProperties for Artist -->

    <!-- Name of the Person -->
    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdfs:label>Name</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of the person.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Birthdate of the Person -->
    <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology/birthDate">
        <rdfs:label>Birth Date</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment>The birthdate of the person.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Define DatatypeProperties for Song -->

    <!-- Name of the Song -->
    <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/name">
        <rdfs:label>Name</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Song"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The title of the film.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Title of the Song -->
    <owl:DatatypeProperty rdf:about="http://dbpedia.org/property/name">
        <rdfs:label>Title</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Song"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of the person.</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Release date of the Song -->
    <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology/releaseDate">
        <rdfs:label>Release Date</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Song"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment>The release date of the song.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="http://dbpedia.org/ontology/artist">
        <rdfs:label>Artist</rdfs:label>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Song"/>
        <rdfs:range rdf:resource="http://dbpedia.org/ontology/Person"/>
        <rdfs:comment>The artist of the song.</rdfs:comment>
    </owl:ObjectProperty>

</rdf:RDF>
